{
  "name": "gotomarket",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/index.ts",
  "scripts": {
    "build": "npm run tsc",
    "tslint": "tslint --project ./tsconfig.json ./src/**/*.ts",
    "test": "mocha",
    "test-coverage": "nyc mocha",
    "integration-test-be": "mocha --opts test/integrationtest.mocha.opts",
    "start-be": "node -r ts-node/register src/server/index.ts",
    "start-fe": "react-scripts start",
    "build-fe": "react-scripts build",
    "test-fe": "react-scripts test --env=jsdom",
    "tslint:watch": "npm run tslint && onchange 'src/**/*.*s' -- npm run tslint",
    "test:watch": "npm test && onchange 'src/**/*.*s' -- npm test",
    "kustomize:dist": "echo '# autogenerated by `npm run kustomize:dist`. Some lines that were split by kustomize have been joined manually.' > kustomize/dist/gtm.yaml && kustomize build kustomize/overlays/dist >> kustomize/dist/gtm.yaml"
  },
  "author": "replicatedhq",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.5",
    "express": "^4.14.0",
    "file-saver": "^2.0.1",
    "moment": "^2.24.0",
    "pino": "^5.11.1",
    "pino-pretty": "^2.5.0",
    "pipedrive": "^6.0.3",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-markdown": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "proxy": "http://backend:3000",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.0.37",
    "@types/node": "^8.10.39",
    "@types/pino": "^5.8.4",
    "@types/request-promise": "^4.1.42",
    "@types/wait-on": "^3.2.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "mocha-jenkins-reporter": "^0.4.1",
    "nyc": "^13.3.0",
    "onchange": "^5.2.0",
    "prettier": "^1.16.3",
    "react-scripts": "^2.1.5",
    "source-map-support": "^0.5.10",
    "ts-node": "^3.3.0",
    "tslint": "^5.12.1",
    "tslint-config-replicated": "^1.0.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typemoq": "^2.1.0",
    "typescript": "^2.9.2",
    "wait-on": "^3.2.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
